QMainWindow {
	background-color:#f3f3f3;
}
QDialog {
	background-color:#f3f3f3;
}
QColorDialog {
	background-color:#f3f3f3;
}
QTextEdit {
    background-color:#f3f3f3;
    
    border-style: solid;
	border-color: #919191;

	border-width: 1px;
	color: #545454;
}
QPlainTextEdit {
	selection-background-color:#007b50;
	background-color:#f3f3f3;
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 1px;
	color: #545454;
}
QPushButton{
	border-style: solid;
     border-radius:5px;
	color: #dfe6e9;
	padding: 2px;
	background-color: #00b894;
}

QToolButton {
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #04b97f;
	border-bottom-width: 1px;
	border-style: solid;
	color: #545454;
	padding: 2px;
	background-color: #f3f3f3;
}
QToolButton:hover{
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #37efba;
	border-bottom-width: 2px;
	border-style: solid;
	color: #545454;
	padding-bottom: 1px;
	background-color: #f3f3f3;
}
QPushButton:hover{
	border-style: solid;
	border-top-color: #37efba;
	border-right-color: #37efba;
	border-left-color: #37efba;
	border-bottom-color: #37efba;
	border-bottom-width: 2px;
	border-style: solid;
	color: #545454;
	padding-bottom: 2px;
	background-color: #f3f3f3;
}
QPushButton:pressed{
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #37efba;
	border-bottom-width: 2px;
	border-style: solid;
	color: #37efba;
	padding-bottom: 1px;
	background-color: #f3f3f3;
}
QPushButton:disabled{
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #808086;
	border-bottom-width: 2px;
	border-style: solid;
	color: #808086;
	padding-bottom: 1px;
	background-color: #f3f3f3;
}
QLineEdit {
	border-width: 1px;
	border-color: #919191;
	border-style: solid;
	padding: 0 8px;
	color: #545454;
	background:#f3f3f3;
	selection-background-color:#007b50;
	selection-color: #f3f3f3;
}
QLabel {
	color: #545454;
}
QLCDNumber {
	color: #37e6b4;
}
QProgressBar {
	text-align: center;
	color: rgb(240, 240, 240);
	border-width: 1px; 
	border-radius: 10px;
	border-color: rgb(58, 58, 58);
	border-style: inset;
	background-color:#f3f3f3;
}
QProgressBar::chunk {
	background-color: #04b97f;
	border-radius: 5px;
}
QMenuBar {
  background-color: #f3f3f3;
  padding: 2px;
  
  color: #545454;
}

QMenuBar:focus {
  border: 1px solid #009dff;
}

QMenuBar::item {
  background: transparent;
  padding: 4px;
}

QMenuBar::item:selected {
  padding: 4px;
  background: transparent;
  border: 0px solid #32414B;
}

QMenuBar::item:pressed {
  padding: 4px;
  border: 0px solid #37efba;
  background-color: #37efba;
  color: rgb(50, 51, 75);
  margin-bottom: 0px;
  padding-bottom: 0px;
}

/* QMenu ------------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenu

--------------------------------------------------------------------------- */
QMenu
{
    background-color: #f3f3f3;
    border: 1px solid #4a5157;
    padding: 10px;
	color: #545454;

}


QMenu::item
{
    background-color: transparent;
    padding: 2px 20px 2px 20px;
	min-width: 200px;

}


QMenu::separator
{
   	background-color: #242424;
	height: 1px;

}


QMenu::item:disabled
{
    color: #555;
    background-color: transparent;
    padding: 2px 20px 2px 20px;

}


QMenu::item:selected
{
	background-color: #37efba;
	border: #37efba;
	color: rgb(49, 49, 66);

}
QTabWidget {
	color:rgb(0,0,0);
	background-color:#f3f3f3;
}
QTabWidget::pane {
		border-color: rgb(77,77,77);
		background-color:#f3f3f3;
		border-style: solid;
		border-width: 1px;
    	border-radius: 6px;
}
QTabBar::tab {
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-bottom-width: 1px;
	border-style: solid;
	color: #808086;
	padding: 3px;
	margin-left:3px;
	background-color: #f3f3f3;
}
QTabBar::tab:selected, QTabBar::tab:last:selected, QTabBar::tab:hover {
  	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #04b97f;
	border-bottom-width: 2px;
	border-style: solid;
	color: #f3f3f3;
	padding-left: 3px;
	padding-bottom: 2px;
	margin-left:3px;
	background-color: #f3f3f3;
}

QCheckBox {
	color: #545454;
	padding: 2px;
}
QCheckBox:disabled {
	color: #808086;
	padding: 2px;
}

QCheckBox:hover {
	border-radius:4px;
	border-style:solid;
	padding-left: 1px;
	padding-right: 1px;
	padding-bottom: 1px;
	padding-top: 1px;
	border-width:1px;
	border-color: rgb(87, 97, 106);
	background-color:#f3f3f3;
}
QCheckBox::indicator:checked {

	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: #04b97f;
	color: #545454;
	background-color: #04b97f;
}
QCheckBox::indicator:unchecked {

	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: #04b97f;
	color: #545454;
	background-color: transparent;
}
QRadioButton {
